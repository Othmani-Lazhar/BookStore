<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="BookStore.Entites.Domain.Pulisher, BookStore.Entites" table="Pulishers">
    <id type="int" column="ID" name="ID">
      <generator class="identity"></generator>
    </id>

    <property name="PulisherName" column="PulisherName" type="string" />
    <property name="Addres" column="Addres" type="string" />
    <property name="Phone" column="Phone" type="string" />
    <property name="WebAddres" column="WebAddres" type="string" />
    <property name="Email" column="Email" type="string" />

  </class>
</hibernate-mapping>